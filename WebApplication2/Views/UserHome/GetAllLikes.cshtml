@model IEnumerable<WebApplication2.Models.ApplicationUser>

<div>
    @if (Model.Count() == 0)
    {
        <p>no likes</p>

    }
    else
    {
        @foreach (var item in Model)

        {
            <div>
                <a asp-action="" asp-route-id="">
                    <img src="~/images/cover.jpg" width="50" height="50">
                    @item.UserName

                </a>
            </div>
        }
    }


</div>


