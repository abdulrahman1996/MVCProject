@model WebApplication2.Models.ApplicationUser
<div class="row">
    <div class="col-md-6 text-center">
        <form asp-action="Edit" data-ajax="true" data-ajax-method="Post" data-ajax-update="#MyGetAll" data-ajax-mode="replace" data-ajax-success="fun2()">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>

            <div class="form-group col-lg-12 col-lg-offset-6">
                <label asp-for="UserName" class="control-label"></label>
                <input type="text" asp-for="UserName" class="form-control" />
                <span asp-validation-for="UserName" class="text-danger"></span>
            </div>
            <div class="form-group col-lg-12 col-lg-offset-6">
                <label asp-for="Gender" class="control-label"></label>
                <input type="text" asp-for="Gender" class="form-control" />
                <span asp-validation-for="Gender" class="text-danger"></span>
            </div>
            <div class="form-group col-lg-12 col-lg-offset-6">
                <label asp-for="PhoneNumber" class="control-label"></label>
                <input type="text" asp-for="PhoneNumber" class="form-control" />
                <span asp-validation-for="PhoneNumber" class="text-danger"></span>
            </div>
            <div class="form-group col-lg-12 col-lg-offset-6">
                <label asp-for="City" class="control-label"></label>
                <input type="text" asp-for="City" class="form-control" />
                <span asp-validation-for="City" class="text-danger"></span>
            </div>
            <div class="form-group col-lg-12 col-lg-offset-6">
                <label asp-for="Country" class="control-label"></label>
                <input type="text" asp-for="Country" class="form-control" />
                <span asp-validation-for="Country" class="text-danger"></span>
            </div>



            <div class="form-group col-lg-12 col-lg-offset-6">
                <input type="submit" value="Save" class="btn btn-warning" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>